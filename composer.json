{
    "name": "leuverink/lockdown",
    "description": "Easily lock sections of your Laravel app with Basic Access Authentication using convenient route middleware & configurable guards",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem Leuverink",
            "email": "willem@leuver.ink",
            "homepage": "https://leuver.ink",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "5.7.* || 5.8.* || 6.*",
        "illuminate/http": "5.7.* || 5.8.* || 6.*",
        "illuminate/routing": "5.7.* || 5.8.* || 6.*",
        "illuminate/support": "5.7.* || 5.8.* || 6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.0 || ~4.0",
        "codedungeon/phpunit-result-printer": "^0.26.2"
    },
    "autoload": {
        "psr-4": {
            "Leuverink\\Lockdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leuverink\\Lockdown\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leuverink\\Lockdown\\ServiceProvider"
            ],
            "aliases": {
                "lockdown": "Leuverink\\Lockdown\\Lockdown"
            }
        }
    }
}
