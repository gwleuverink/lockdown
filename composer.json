{
    "name": "gwleuverink/laravel-lockdown",
    "description": "Easily lock sections of your Laravel app with Basic Access Authentication using convenient route middleware & configurable guards",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem Leuverink",
            "email": "willem@leuver.ink"
        }
    ],
    "minimum-stability": "stable",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Gwleuverink\\Lockdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gwleuverink\\Lockdown\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gwleuverink\\Lockdown\\ServiceProvider"
            ],
            "aliases": {
                "lockdown": "Gwleuverink\\Lockdown\\Lockdown"
            }
        }
    }
}
